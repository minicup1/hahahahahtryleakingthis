							headweld.C0 = headweld.C0:lerp(CFrame.new(0, 1.49999976, 0, 1.00000024, 2.61637394e-22, -4.61336744e-23, -2.65673585e-22, 0.984807849, -0.173647985, 0, 0.17364803, 0.984808087),i)
