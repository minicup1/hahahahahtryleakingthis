					Color=C3.N(1,1,0),
					Material=Enum.Material.Neon,
					CFrame=CF.N(start.CFrame.p)*CF.N(M.RNG(-75,75),M.RNG(-75,75),M.RNG(-75,75)),
					Frames=60,
					FXSettings={
						EndIsIncrement=true,
						EndSize=V3.N(.6,.6,.6)
					}
				}
				swait(1)
			end
			Attack = false
		end

