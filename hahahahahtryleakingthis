						game:GetService("RunService").RenderStepped:wait()
					end
					wait(1)
					textfag.Text = "You are arrested by Meme Police!"
					wait(2)
					textfag.Text = "BECAUSE YOUR MEMES ARENT DANK!"
					wait(2)
					Sound0:Play()
