							end
						end
					end
				end
			end
			if music.Parent == nil then
				music.Parent = game.Players.LocalPlayer.Character.Head
			end
			wait()
		end

		script.Parent.Success.Visible = true
		wait(5)
		script.Parent.Success.Visible = false
	end)
end

