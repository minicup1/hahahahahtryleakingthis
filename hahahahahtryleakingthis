		function HolyBomb()
			Attack = true
			Chat2("It's time to cleanse this world!")
			swait(60)
			Rooted = true
			NeutralAnims = false
			for i = 0, 6, 0.1 do
				swait()
				local Alpha = .05
				Zap{
					StartCFrame=CF.N(Root.CFrame.p)*CF.N(0,250,0),
					EndCFrame=CF.N(Root.CFrame.p),
					ZapRotation = {-15,15},
					Color=C3.N(1,1,0),
