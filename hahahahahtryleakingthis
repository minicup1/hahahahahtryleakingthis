			if v:IsA("Accessory") then
				if hatnameclone[v.Name] then
					if hatnameclone[v.Name] == "s" then
						hatnameclone[v.Name] = {}
					end
					table.insert(hatnameclone[v.Name],v)
				else
					hatnameclone[v.Name] = "s"
				end
			end
		end
