						if WeirdOrbital then
							game.Players.LocalPlayer.Character.Head.CanCollide = false
							game.Players.LocalPlayer.Character.Torso.CanCollide = false
							game.Players.LocalPlayer.Character["Left Leg"].CanCollide = false
