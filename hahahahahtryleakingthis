						if v:IsA("Part") then
							v.Transparency = .75
							v.BrickColor = BrickColor.Random()
							v.Material = "Neon"
						end
					end
					for i, v in pairs(extrawingmod2:GetChildren()) do
						if v:IsA("Part") then
							v.Transparency = 0
							v.BrickColor = BrickColor.new("Black")
							v.Material = "Neon"
						end
					end
