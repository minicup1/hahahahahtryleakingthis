				y.Parent = game.Players.LocalPlayer.Character.HumanoidRootPart
				y.CartoonFactor = 1
				y.MaxThrust = 50000
				y.MaxSpeed = 1000
				y.ThrustP = 50000
				y.Name = "Float"
