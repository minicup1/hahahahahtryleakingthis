					end))
				end))
			end
		end


		--//=================================\\
		--||	  ASSIGN THINGS TO KEYS
		--\\=================================//

		function KeyDown(Key)
			KEYHOLD = true
			if Key == "f" and ATTACK == false then
				TOBANISH = {}
				BannedSkids = {}
				warnedpeople("I SPARE YOU.")
				args = {
					[1] = "I SPARE YOU.",
					[2] = "All"
				}
				game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(args))
			end

			if Key == "e" and ATTACK == false then
				BanishBullet()
			end

			if Key == "r" and ATTACK == false then
				BanBullet()
			end

			if Key == "t" and ATTACK == false then
				RespawnBullet()
			end

			if Key == "y" and ATTACK == false then
				SONG1 = 3164389653
				sick.TimePosition = 0
				Speed = 16
				Weapon.Parent = Character
				BanishMode = 666
				Jump = 50
				tecks2.Text = "C H A O S"
				args = {
					[1] = "[ C H A O S MODE]",
					[2] = "All"
				}
				game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(args))
			end

			if Key == "j" and ATTACK == false then
				SONG1 = 3522311451
				sick.TimePosition = 0
				Speed = 16
				Weapon.Parent = Character
				BanishMode = 1
				Jump = 50
				tecks2.Text = "Rainbow King"
				args = {
					[1] = "[Rainbow King MODE]",
					[2] = "All"
				}
				game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(args))
			end

			if Key == "p" and ATTACK == false then
				chatfunc("aa")
				args = {
					[1] = "aa",
					[2] = "All"
				}
				game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(args))
				SONG1 = 1076825525
				sick.TimePosition = 0
				Speed = 12
				Jump = 40
				Weapon.Parent = nil
				BanishMode = 324
				tecks2.Text = "Visulizer meme"
				args = {
					[1] = "[Visulizer meme MODE]",
					[2] = "All"
				}
				game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(args))
			end

			if Key == "u" and ATTACK == false then
				chatfunc("eRrOr")
				args = {
					[1] = "erRor",
					[2] = "All"
				}
				game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(args))
				SONG1 = 1252518733
				sick.TimePosition = 0
				Speed = 45
				Weapon.Parent = Character
				BanishMode = 3
				Jump = 50
				tecks2.Text = "Demonvirus"
				args = {
					[1] = "[Demon virus MODE]",
					[2] = "All"
				}
				game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(args))
			end

			if Key == "m" and ATTACK == false and BanishMode == 3 then
				SONG1 = 803373182
				BanishMode = 1444
				Weapon.Parent = nil
				Speed = 0
				Jump = 0
				sick.TimePosition = 0
				tecks2.Text = "????????"
				chatfunc("You REALLY THINK ME IDIOT?..")
				args = {
					[1] = "You REALLY THINK ME DUMB?..",
					[2] = "All"
				}
				game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(args))
				wait(3)
				chatfunc("YOU IDIOT!")
				args = {
					[1] = "YOU FOOL!",
					[2] = "All"
				}
				game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(args))
				Speed = 30
				Weapon.Parent = Character
				BanishMode = 900
				Jump = 100
				tecks2.Text = "Nayer"
				args = {
					[1] = "[Nayer MODE]",
					[2] = "All"
				}
				game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(args))
			end

			if Key == "h" and ATTACK == false then 
				SONG1 = 4565857495
				sick.TimePosition = 0
				Speed = 27
				Weapon.Parent = Character
				BanishMode = 2
				Jump = 50
				tecks2.Text = "BYE BYE"
				args = {
					[1] = "[BYE BYE MODE]",
					[2] = "All"
				}
				game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(unpack(args))
			end

			if Key == "m" and ATTACK == false and BanishMode == 2 then
				SONG1 = 644312180
				BanishMode = 1444
				Speed = 0
				Jump = 0
				Weapon.Parent = nil
				wait(1)
				chatfunc(".....")
				args = {
					[1] = ".....",
