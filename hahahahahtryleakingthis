							rhandweld.C0 = rhandweld.C0:lerp(CFrame.new(1.59805202, 0.118935108, 0.435078144, 0.984808207, -0.173647344, -9.4095941e-07, 0.111619018, 0.63302201, 0.766044796, -0.133021042, -0.754407227, 0.642787576),i)
