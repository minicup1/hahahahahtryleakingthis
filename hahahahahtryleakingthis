							if canwalk then
								--macheteweld.C0 = macheteweld.C0:lerp(CFrame.new(-0.0285909176, -0.964775562, -1.62501633, 2.65673535e-22, 1, 5.23747954e-22, 1, -2.65673535e-22, -3.13253081e-22, -3.13253081e-22, 5.23747954e-22, -1),i)
