				t5.CartoonFactor = 1
				t5.Name = "fivee"
				local t6 = Instance.new("RocketPropulsion")
				t6.Parent = game.Players.LocalPlayer.Character.HumanoidRootPart
				t6.TurnP = 100000
				t6.MaxThrust = 100000
				t6.MaxSpeed = 5000
				t6.ThrustP = 100000
				t6.CartoonFactor = 1
				t6.Name = "sixx"
				for i,v in pairs(GetPlayer(TXTBOX_PlrName2.Text))do
